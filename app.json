{
  "expo": {
    "name": "Borealis",
    "slug": "borealis",
    "version": "1.0.0",
    "scheme": "borealis",
    "ios": {
      "bundleIdentifier": "com.borealis.borealis",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "${EXPO_PUBLIC_GOOGLE_CLIENT_ID}"
            ]
          }
        ]
      }
    },
    "android": {
      "package": "com.borealis.borealis"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "@react-native-google-signin/google-signin"
    ],
    "extra": {
      "eas": {
        "projectId": "${EAS_PROJECT_ID}"
      }
    },
    "owner": "${EXPO_OWNER}"
  }
}
